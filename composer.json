{
    "name": "wayen/azuread",
    "description": "This is base on Azure auth for saml 2.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "wayen zhuo",
            "email": "wayenzhuo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-openssl": "*",
        "ext-dom": "*",
        "ext-mcrypt": "*"
    },
    "autoload": {
        "psr-4": {
            "wayen\\azuread\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "wayen\\azuread\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    }
}